{
  "name": "factful-design-system",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "types": "./src/index.tsx",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "generate:component": "turbo gen react-component"
  },
  "devDependencies": {
    "@factful/eslint-config": "workspace:*",
    "@factful/typescript-config": "workspace:*",
    "@turbo/gen": "^1.12.4",
    "@types/node": "^20.11.24",
    "@types/eslint": "^8.56.5",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "tsup": "^8.0.2"
  },
  "dependencies": {
    "react": "^18.2.0"
  }
}
