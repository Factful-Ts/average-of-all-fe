{
  "extends": "@factful/typescript-config/nextjs.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/public/*": [
        "../public/*"
      ],
      "@/*": [
        "*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "next.config.js",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
